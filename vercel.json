{
  "version": 2,

  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/scrape",
      "schedule": "25 12 * * *"
    },
    {
      "path": "/api/scrape",
      "schedule": "0 21 * * *"
    }
  ]
}
